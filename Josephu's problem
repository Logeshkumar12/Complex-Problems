def Josh(person, k, index):
    if len(person) == 1:
        print(person[0])
        return
    
    index = ((index+k)%len(person))
    print("Count:" ,len(person))
    print("index :",index)
    person.pop(index)
    print(person)
    
    Josh(person,k,index)


n = int(input("No of Persons: "))
k = int(input("Skip value: "))
k-=1

index = 0
person=[]
for i in range(1,n+1):
    person.append(i)
Josh(person,k,index) 
